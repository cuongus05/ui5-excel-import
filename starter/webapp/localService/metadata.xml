<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<edmx:Edmx Version="1.0"
    xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx">
    <edmx:DataServices
        xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata" m:DataServiceVersion="1.0">
        <Schema Namespace="Product.Models"
            xmlns:d="http://schemas.microsoft.com/ado/2007/08/dataservices"
            xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
            xmlns="http://schemas.microsoft.com/ado/2006/04/edm">
            <EntityType Name="Product">
                <Key>
                    <PropertyRef Name="ProductId" />
                </Key>
                <Property Name="ProductId" Type="Edm.String" Nullable="false" />
                <Property Name="Category" Type="Edm.String" Nullable="true" />
                <Property Name="MainCategory" Type="Edm.String" Nullable="true" />
                <Property Name="TaxTarifCode" Type="Edm.String" Nullable="true" />
                <Property Name="SupplierName" Type="Edm.String" Nullable="true" />
                <Property Name="WeightMeasure" Type="Edm.Double" Nullable="true" />
                <Property Name="WeightUnit" Type="Edm.String" Nullable="true" />
                <Property Name="Description" Type="Edm.String" Nullable="true" />
                <Property Name="Name" Type="Edm.String" Nullable="true" />
                <Property Name="DateOfSale" Type="Edm.DateTime" Nullable="false" />
                <Property Name="Status" Type="Edm.String" Nullable="false" />
                <Property Name="Quantity" Type="Edm.Int32" Nullable="false" />
                <Property Name="UoM" Type="Edm.String" Nullable="true" />
                <Property Name="CurrencyCode" Type="Edm.String" Nullable="false" />
                <Property Name="Price" Type="Edm.Double" Nullable="false" />
                <Property Name="Width" Type="Edm.Double" Nullable="true" />
                <Property Name="Depth" Type="Edm.Double" Nullable="true" />
                <Property Name="Height" Type="Edm.Double" Nullable="true" />
                <Property Name="DimUnit" Type="Edm.String" Nullable="true" />
            </EntityType>
            <EntityContainer Name="Products" m:IsDefaultEntityContainer="true">
                <EntitySet Name="Products" EntityType="Product.Models.Product" />
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>